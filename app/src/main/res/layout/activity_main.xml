<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#191919"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/logoname"
        android:layout_width="167dp"
        android:layout_height="130dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="370dp"
        android:layout_marginEnd="175dp"
        android:layout_marginBottom="265dp"
        android:fontFamily="@font/bangers"
        android:transitionName="logoname_trans"
        android:text="@string/eat_it"
        android:textColor="#FAF7F7"
        android:textColorHighlight="#131212"
        android:textSize="70sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.37" tools:targetApi="lollipop" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="150dp"
        android:layout_marginBottom="211dp"
        android:contentDescription="@string/todo"
        android:transitionName="logo_trans"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/burgerlogo" tools:targetApi="lollipop"/>
</androidx.constraintlayout.widget.ConstraintLayout>